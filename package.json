{
  "name": "edfinder",
  "version": "1.0.0",
  "description": "Build a finder with electron",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnzoDeg40/EdFinder.git"
  },
  "author": "Enzo Degraeve",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EnzoDeg40/EdFinder/issues"
  },
  "homepage": "https://github.com/EnzoDeg40/EdFinder#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^18.2.3"
  },
  "dependencies": {
    "custom-electron-titlebar": "^4.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "username": "^6.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "edfinder"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
